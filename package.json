{
  "name": "sentry-poc",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "pre-build": "bash pre-build.sh",
    "upload-sourcemaps": "sentry-cli releases files $VERSION upload-sourcemaps ./dist --ext map --rewrite",
    "clean-sourcemaps": "find ./dist -name '*.map' -delete",
    "finalize-release": "sentry-cli releases finalize $VERSION",
    "build:deploy": "npm run build && npm run pre-build && npm run upload-sourcemaps && npm run clean-sourcemaps && npm run finalize-release"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.2",
